{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": "http://localhost:5173"
  },
  "Auth": {
    "Provider": "Jwt",
    "Jwt": {
      "Issuer": "retailfixit-dev",
      "Audience": "retailfixit-dev-audience",
      "SecretKey": "dev-secret-key-minimum-32-characters-long!!"
    },
    "AzureAd": {
      "Instance": "https://login.microsoftonline.com/",
      "TenantId": "",
      "ClientId": ""
    }
  },
  "CosmosDb": {
    "Endpoint": "",
    "AccountKey": "",
    "DatabaseName": "RetailFixIT"
  },
  "SignalR": {
    "Provider": "InMemory",
    "AzureSignalR": {
      "ConnectionString": ""
    }
  },
  "Messaging": {
    "Provider": "InMemory",
    "AzureServiceBus": {
      "ConnectionString": "",
      "TopicPrefix": "retailfixit"
    }
  },
  "AI": {
    "Provider": "Gemini",
    "Gemini": {
      "ApiKey": "YOUR_GEMINI_API_KEY_HERE",
      "Model": "gemini-1.5-flash"
    },
    "AzureOpenAI": {
      "Endpoint": "",
      "ApiKey": "",
      "DeploymentName": "gpt-4o"
    }
  },
  "Cache": {
    "Provider": "Memory",
    "Redis": {
      "ConnectionString": ""
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
